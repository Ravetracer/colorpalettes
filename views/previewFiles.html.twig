{% extends 'layout.html.twig' %}

{% block mainContent %}
    <div class="row">
        <div class="large-12 columns">
            <h1>Color palette preview files</h1>
        </div>
    </div>

    <ul>
        {% for file in files %}
            <li><a href="/import/preview/{{ file|url_encode }}" target="_blank">{{ file }}</a></li>
        {% endfor %}
    </ul>
{% endblock %}
